function[]=progressbar(k,n)


    clc
    fprintf('0','%')
    fprintf('[')
    p=floor(k/50);
    for j=1:floor(n/p)
        fprintf('-')
    end
    
    fprintf('>%2.1g',(100*n/k))
    for j=1:(floor(k/p)-floor(n/p))
        
        fprintf(' ')
    end
    if (n/k)<10
        fprintf(' ')
    end
    fprintf(']100')
   
    

